			$('#btn_expand').hide();
			$('#btn_collapse').show();
		});
		
		$('#btn_collapse').click(function()
		{
			//$('#collapsable_1').slideUp();
			$('#collapsable_1').hide();
			$('#btn_expand').show();
			$('#btn_collapse').hide();
		});
		
	}

	$('.researchchair p').hide();
	
	$('.researchchair div').click(function() {
		
		if($(this).hasClass('open'))
		{
			$(this).removeClass('open');
			$(this).next().slideUp(200);
		}
		else
		{
			$(this).addClass('open');
			$(this).next().slideDown(200);
		}
	
	});
	
});
//]]>
